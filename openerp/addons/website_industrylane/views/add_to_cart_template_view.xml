<template id="spend_management" name="Spend Management">
<!---


select partner_id,amount_total,date_invoice from account_invoice
First Filter get 
month start date and end date

select ai.partner_id,ai.amount_total,ai.date_invoice, to_char(ai.date_invoice, 'MONTH') from account_invoice ai
	INNER JOIN account_invoice_line ail on ai.id = ail.invoice_id 

-->
